<%- include('layout', { content: `
  <div class="m-8 p-4">
    <h2 class="text-2xl font-semibold mb-4">Agregar Vehículo</h2>
    <form action="/add-vehicle" method="post" class="space-y-4">
      ${typeof message !== 'undefined' ? `
        <div>
          <h3 class="text-2xl font-semibold mb-4 text-green-500">${message}</h3>
        </div>
        ` : ''
      }
      <div>
        <label for="plates" class="block font-semibold">Placas:</label>
        <input type="text" id="plates" name="plates" class="w-full border rounded-md p-2">
      </div>
      <div>
        <label for="brand" class="block font-semibold">Marca:</label>
        <input type="text" id="brand" name="brand" class="w-full border rounded-md p-2">
      </div>
      <div>
        <label for="model" class="block font-semibold">Modelo:</label>
        <input type="text" id="model" name="model" class="w-full border rounded-md p-2">
      </div>
      <div>
        <label for="motor" class="block font-semibold">Motor:</label>
        <input type="text" id="motor" name="motor" class="w-full border rounded-md p-2">
      </div>
      <div>
        <button type="submit" class="bg-blue-500 hover:bg-blue-600 text-white font-semibold py-2 px-4 rounded-md">Agregar Vehículo</button>
        <button type="button" class="bg-gray-300 hover:bg-gray-400 text-gray-600 font-semibold py-2 px-4 rounded-md ml-2">Cancelar</button>
      </div>
    </form>
  </div>

  `,
  user: user,
  isAuthenticated: isAuthenticated

  }) %>
